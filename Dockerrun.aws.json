{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "mataycode-website",
      "image": "matayyy/mataycode-website",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://TO_DO_DATABASE_URL:5432/customer"
        }
      ]
    }
  ]
}